<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ccproject.test.myslidetest" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.ccproject.cloud.cloudclubbing.ApplicationController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ccproject.cloud.cloudclubbing.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.ccproject.cloud.cloudclubbing.SplashActivity"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ccproject.cloud.cloudclubbing.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
    </application>

</manifest>
